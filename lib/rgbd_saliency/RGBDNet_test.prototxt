    lr_mult: 0
  }
  param {
    lr_mult: 0
  }
  param {
    lr_mult: 0
  }
}
layer {
  name: "low_relu1"
  type: "ReLU"
  bottom: "region_conv1"
  top: "region_conv1"
}

layer {
