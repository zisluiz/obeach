layer {
  name: "genslic"
  type: "GenSlicinfo"
  bottom: "data"
  top: "slic"
  top: "deadmap"
  gen_slicinfo_param {
    image_size: 324
    slic_size: 18
    coh_weight: 1.4
    cielab: true 
    min_pixel_num: 100
  }
}

layer {
  name: "initial_low"
  type: "LowlevelDistance"
  bottom: "data"
  bottom: "slic"
  bottom: "deadmap"
  top: "region_features"
  top: "region_labels"
  top: "query_index_cue" # order of picked region   
  include {
    phase: TRAIN
  }
  lowlevel_distance_param {
    feature_option: 3
    grid_size: 20
    train_region_per_image: 50
    spixel_size: 18
  }
}

layer {
  name: "initial_low"
  type: "LowlevelDistance"
  bottom: "data"
  bottom: "slic"
  bottom: "deadmap"
  top: "region_features"
  top: "region_labels"
  top: "query_index_cue" # order of picked region   
  include {
    phase: TEST
  }
  lowlevel_distance_param {
    feature_option: 3
    grid_size: 20
    train_region_per_image: 0 # this means train all regions
    spixel_size: 18
  }
}

layer {
  name: "low_conv1"
  type: "Convolution"
  bottom: "region_features"
